name: "vendors"
nodes: {
  name: "ceos"
  vendor: ARISTA
  model: "ceos"
  os: "eos"
  config: {
    image: "us-west1-docker.pkg.dev/gep-kne/arista/ceos:ga"
    config_path: "/mnt/flash"
    config_file: "startup-config"
    file: "ceos.cfg"
    cert: {
      self_signed: {
        cert_name: "gnmiCert.pem"
        key_name: "gnmiCertKey.pem"
        key_size: 4096
      }
    }
  }
}
nodes: {
  name: "cptx"
  vendor: JUNIPER
  model: "cptx"
  os: "evo"
  config: {
    image: "us-west1-docker.pkg.dev/gep-kne/juniper/cptx:ga"
    config_path: "/home/evo/configdisk"
    config_file: "juniper.conf"
    file: "cptx.cfg"
    cert: {
      self_signed: {
        cert_name: "grpc-server-cert"
        key_name: "N/A"
        key_size: 4096
      }
    }
  }
  interfaces: {
    key: "eth1"
    value: {
      name: "et-0/0/0"
    }
  }
  interfaces: {
